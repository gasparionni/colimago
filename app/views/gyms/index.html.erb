<div class="wrapper">
  <div class="header">
    <nav>
      <ul>
        <li><%= link_to 'Inicio', root_path %></li>
        <li><%= link_to 'Gimnasios', gyms_path, {:class=>"active"} %></li>
        <li><%= link_to 'PokÃ©paradas', pokestops_path %></li>
      </ul>
    </nav>
  </div>
  <div class="content">
    <h1>Gimnasios</h1>
    <table class="center">
      <tr>
        <th>Nombre</th>
        <th>Latitud</th>
        <th>Longitud</th>
        <th colspan="2">Acciones</th>
      </tr>
    
      <% @gyms.each do |gym| %>
        <tr>
          <td><%= gym.name %></td>
          <td><%= gym.latitude %></td>
          <td><%= gym.longitude %></td>
          <td><%= link_to 'Show', gym_path(gym) %></td>
          <td><%= link_to 'Edit', edit_gym_path(gym) %></td>
        </tr>
      <% end %>
    </table>
  </div>
  <div class="footer">
    Footer
  </div>
</div>
